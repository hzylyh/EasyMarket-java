<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.easymarket.dao.VisitDetailDao" >

    <insert id="insertVisitInfo" parameterType="com.easymarket.entity.VisitDetailEntity">
        insert into em_visit_detail(templateId, comeSite, visitTime)
        values
        (#{templateId}, #{comeSite}, #{visitTime})
    </insert>

</mapper>